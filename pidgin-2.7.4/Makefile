	cd $(top_builddir) && $(SHELL) ./config.status $@
libpurple/plugins/perl/common/Makefile.PL: $(top_builddir)/config.status $(top_srcdir)/libpurple/plugins/perl/common/Makefile.PL.in
	cd $(top_builddir) && $(SHELL) ./config.status $@
